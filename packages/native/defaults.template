<%= storyName %>.args = {
    <% _.each(controls, function(control){ %>
    <% if (control[1] === '') { %>
    <%= control[0] %>: "Default",
    <% } else if (typeof control[1] == 'boolean') { %>
    <%= control[0] %>: <%= control[1] %>,
    <% } else if (!control[1].hasOwnProperty('min') && !control[1].hasOwnProperty('max')) { %>
    <%= control[0] %>: "<%= control[1][0] %>",
    <% } %>
    <% }); %>
};
